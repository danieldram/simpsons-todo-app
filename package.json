  "devDependencies": {
    "babel-jest": "^19.0.0",
    "babel-plugin-transform-async-to-generator": "^6.22.0",
    "babel-polyfill": "^6.23.0",
    "node-sass": "^4.5.0",
    "react-scripts": "0.9.3"
  },
  "dependencies": {
